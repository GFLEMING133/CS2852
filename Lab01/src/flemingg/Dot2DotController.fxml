<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="600.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="flemingg.Dot2DotController">
   <children>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="File">
               <items>
                  <MenuItem fx:id="open" mnemonicParsing="false" text="Open" />
                  <MenuItem fx:id="close" mnemonicParsing="false" onAction="#close" text="Close" />
               </items></Menu>
            <Menu mnemonicParsing="false" text="Draw">
              <items>
                <MenuItem fx:id="lines" mnemonicParsing="false" text="Lines Only" />
                  <MenuItem fx:id="dots" mnemonicParsing="false" text="Dots Only" />
              </items>
            </Menu>
        </menus>
      </MenuBar>
      <Canvas fx:id="screenCanvas" height="510.0" width="748.0" />
      <Pane prefHeight="200.0" prefWidth="200.0">
         <children>
            <Text layoutX="14.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </Pane>
   </children>
</VBox>
